{
  "name": "A barcode generator",
  "version": "1.0",
  "description": "A barcode generator Extension!",
  "browser_action": {
    "default_title": "barcode",
    "default_icon": {
      "16": "bicons/icon16.png",
      "32": "bicons/icon32.png",
      "48": "bicons/icon48.png"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "permissions": ["tabs"]
}
